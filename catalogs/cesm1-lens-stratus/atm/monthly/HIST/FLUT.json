{
  "type": "Collection",
  "id": "CESM1LE-atm-HIST-monthly-FLUT",
  "stac_version": "1.0.0",
  "description": "CESM1 Large Ensemble dataset",
  "links": [
    {
      "rel": "RelType.ROOT",
      "href": "../catalog.json",
      "type": "MediaType.JSON"
    },
    {
      "rel": "license",
      "href": "https://www.cesm.ucar.edu/models/cesm2/copyright.html",
      "type": "text/html",
      "title": "CESM2 Copyright & Terms of Use"
    }
  ],
  "stac_extensions": [
    "https://stac-extensions.github.io/datacube/v1.0.0/schema.json"
  ],
  "cube:dimensions": {
    "time": {
      "type": "temporal",
      "description": "time",
      "extent": ["1850-01-16T12:00:00Z", "1919-12-16T12:00:00Z"]
    }
  },
  "cube:variables": {
    "FLUT": {
      "type": "data",
      "description": "Upwelling longwave flux at top of model",
      "dimensions": ["time", "lat", "lon"],
      "unit": "W/m2",
      "shape": [840, 192, 288],
      "chunks": [576, 192, 288],
      "attrs": {
        "Sampling_Sequence": "rad_lwsw",
        "cell_methods": "time: mean",
        "long_name": "Upwelling longwave flux at top of model",
        "units": "W/m2"
      }
    },
    "member_id": {
      "type": "auxiliary",
      "dimensions": [],
      "shape": [],
      "attrs": {}
    },
    "time_bnds": {
      "type": "auxiliary",
      "description": "time interval endpoints",
      "dimensions": ["time", "nbnd"],
      "shape": [840, 2],
      "chunks": [840, 2],
      "attrs": {
        "long_name": "time interval endpoints"
      }
    }
  },
  "providers": [
    {
      "name": "National Center for Atmospheric Research (NCAR)",
      "roles": ["producer", "licensor", "processor"],
      "url": "https://www.cesm.ucar.edu/projects/community-projects/LENS2/"
    }
  ],
  "assets": {
    "zarr-ncar-stratus-s3": {
      "href": "s3://ncar-cesm-lens/atm/monthly/cesmLE-HIST-FLUT.zarr",
      "type": "application/vnd+zarr",
      "xarray:open_kwargs": {
        "use_cftime": true,
        "consolidated": true
      },
      "roles": ["data", "zarr", "ncar-stratus-s3"]
    }
  },
  "stac_collections": [
    "https://stac-extensions.github.io/datacube/v1.0.0/schema.json"
  ],
  "extent": {
    "spatial": {
      "bbox": [[-180, -90, 180, 90]]
    },
    "temporal": {
      "interval": [["1850-01-16T12:00:00Z", "1919-12-16T12:00:00Z"]]
    }
  },
  "license": "CC0-1.0"
}
